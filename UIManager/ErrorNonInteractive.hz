Class	name: #ErrorNonInteractive;	superclass: #ClassError;	instanceSpecification: #(#pointers #words );	instanceVariables: #(#exception );	classVariables: #();	package: #UIManager.Class ErrorNonInteractive >> exception[^ exception]Class ErrorNonInteractive >> exception: anError[exception := anError]Class ErrorNonInteractive >> defaultAction[	" log error and quit "^ UIManager default quitFrom: self signalerContext withMessage: self description]Class ErrorNonInteractive >> description[^ 'An attempt to use interactive tools detected, while in non-interactive mode']Metaclass	name: #ErrorNonInteractive;	instanceVariables: #().Metaclass ErrorNonInteractive >> signalForException: anError[^ self new	exception: anError;	signal]