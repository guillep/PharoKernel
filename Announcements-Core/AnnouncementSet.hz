Class
	name: #AnnouncementSet;
	superclass: #ClassSet;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #();
	classVariables: #();
	package: #'Announcements-Core'.

Class AnnouncementSet >> handles: anAnnouncementClass
[
^ self anySatisfy: [ :each | each handles: anAnnouncementClass ]
]

Class AnnouncementSet >> , anAnnouncementClass
[
self add: anAnnouncementClass
]

Metaclass
	name: #AnnouncementSet;
	instanceVariables: #().

