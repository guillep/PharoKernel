Class	name: #MessageCatcher;	superclass: #ClassProtoObject;	instanceSpecification: #(#pointers #words );	instanceVariables: #(#accumulator );	classVariables: #();	package: #'Kernel-Methods'.Class MessageCatcher >> doesNotUnderstand: aMessage[accumulator ifNotNil: [ accumulator add: aMessage ].^ aMessage]Class MessageCatcher >> privAccumulator[^ accumulator]Class MessageCatcher >> privAccumulator: collection[accumulator := collection]Metaclass	name: #MessageCatcher;	instanceVariables: #().