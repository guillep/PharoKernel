Class Class >> basicEnvironment
[
^ environment
]

Class Class >> basicSubclasses: anArray
[
subclasses := anArray
]

Class Class >> basicSharedPools
[
	"Answer nil or a Set of the pool dictionaries declared in the receiver."
^ sharedPools
]

Class Class >> basicClassPool
[
	"Answer nil or the dictionary of class variables."
^ classPool
]

Class Class >> basicSubclasses
[
^ subclasses
]

