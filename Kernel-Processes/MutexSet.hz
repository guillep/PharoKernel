Class	name: #MutexSet;	superclass: #ClassObject;	instanceSpecification: #(#pointers #words );	instanceVariables: #(#array );	classVariables: #();	package: #'Kernel-Processes'.Class MutexSet >> critical: aBlock[	"Evaluate aBlock aquiring all mutexes"^ self pvtCritical: aBlock startingAt: 1]Class MutexSet >> pvtCritical: aBlock startingAt: index[| mutex |index > array size	ifTrue: [ ^ aBlock value ].mutex := array at: index.^ mutex critical: [ self pvtCritical: aBlock startingAt: index + 1 ]]Class MutexSet >> withAll: mutexList[array := mutexList]Metaclass	name: #MutexSet;	instanceVariables: #().Metaclass MutexSet >> withAll: mutexList[^ self new withAll: mutexList]