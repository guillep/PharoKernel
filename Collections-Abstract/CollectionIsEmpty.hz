Class	name: #CollectionIsEmpty;	superclass: #ClassError;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Collections-Abstract'.Class CollectionIsEmpty >> messageText[	"Overwritten to initialiaze the message text to a standard text if it has not yet been set"^ messageText ifNil: [ messageText := self standardMessageText ]]Class CollectionIsEmpty >> collection[	"Return the collection where something is not found in"^ self signaler]Class CollectionIsEmpty >> standardMessageText[	"Generate a standard textual description"^ String	streamContents: [ :stream | 		stream print: self collection.		stream << ' is empty' ]]Class CollectionIsEmpty >> collection: aCollection[	"Set the collection where something is not found in"self signaler: aCollection]Metaclass	name: #CollectionIsEmpty;	instanceVariables: #().Metaclass CollectionIsEmpty >> signalWith: aCollection[^ self new	collection: aCollection;	signal]