Class	name: #UnixResolver;	superclass: #ClassPlatformResolver;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'FileSystem-Core-Implementation'.Class UnixResolver >> desktop[^ self home / 'Desktop']Class UnixResolver >> home[	"FIXME: hackish solution. should use env vars"^ (self resolveString: self class primitiveGetUntrustedUserDirectory) parent parent parent]Class UnixResolver >> preferences[^ self home / '.config']Class UnixResolver >> documents[^ self home / 'Documents']Metaclass	name: #UnixResolver;	instanceVariables: #().Metaclass UnixResolver >> platformName[^ 'unix']