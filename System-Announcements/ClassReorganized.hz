Class
	name: #ClassReorganized;
	superclass: #ClassSystemAnnouncement;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #(#classReorganized );
	classVariables: #();
	package: #'System-Announcements'.

Class ClassReorganized >> classAffected
[
^ self classReorganized
]

Class ClassReorganized >> classReorganized
[
^ classReorganized
]

Class ClassReorganized >> classReorganized: anObject
[
classReorganized := anObject
]

Metaclass
	name: #ClassReorganized;
	instanceVariables: #().

Metaclass ClassReorganized >> class: aClass
[
^ self new	classReorganized: aClass;	yourself
]

