Class
	name: #ClassRemoved;
	superclass: #ClassSystemAnnouncement;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #(#categoryName #classRemoved );
	classVariables: #();
	package: #'System-Announcements'.

Class ClassRemoved >> categoryName
[
^ categoryName
]

Class ClassRemoved >> categoryName: anObject
[
categoryName := anObject
]

Class ClassRemoved >> classAffected
[
^ self classRemoved
]

Class ClassRemoved >> classRemoved
[
^ classRemoved
]

Class ClassRemoved >> classRemoved: anObject
[
classRemoved := anObject
]

Metaclass
	name: #ClassRemoved;
	instanceVariables: #().

Metaclass ClassRemoved >> class: aClass category: aCategoryName
[
^ self new	classRemoved: aClass;	categoryName: aCategoryName;	yourself
]

