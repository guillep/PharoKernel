Class	name: #NotYetImplemented;	superclass: #ClassSelectorException;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Kernel-Exceptions'.Class NotYetImplemented >> standardMessageText[	"Generate a standard textual description"^ String	streamContents: [ :stream | 		stream print: self selector.		stream << ' is not yet implemented in '.		stream print: self signaler class ]]Metaclass	name: #NotYetImplemented;	instanceVariables: #().