Class	name: #FLNegative8SmallIntegerCluster;	superclass: #ClassFLSmallIntegerCluster;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Fuel-Clusters'.Class FLNegative8SmallIntegerCluster >> materializeInstanceWith: aDecoder[^ aDecoder nextEncodedUint8 negated]Class FLNegative8SmallIntegerCluster >> serializeInstance: anInteger with: anEncoder[anEncoder encodeUint8: anInteger abs]Metaclass	name: #FLNegative8SmallIntegerCluster;	instanceVariables: #().