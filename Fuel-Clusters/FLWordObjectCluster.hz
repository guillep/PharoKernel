Class	name: #FLWordObjectCluster;	superclass: #ClassFLBitsObjectCluster;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Fuel-Clusters'.Class FLWordObjectCluster >> materializeInstanceWith: aDecoder[| inst wideSize |wideSize := aDecoder nextEncodedPositiveInteger.inst := theClass basicNew: wideSize.aDecoder nextEncodedWordsInto: inst.aDecoder isBigEndian = Smalltalk isBigEndian asBit	ifTrue: [ ^ inst ]	ifFalse: [ ^ self swapBytesOf: inst ]]Class FLWordObjectCluster >> swapBytesOf: aWordObject[Bitmap swapBytesIn: aWordObject from: 1 to: aWordObject basicSize.^ aWordObject]Class FLWordObjectCluster >> serializeInstance: aWordObject with: anEncoder[anEncoder encodePositiveInteger: aWordObject basicSize.anEncoder encodeWords: aWordObject]Metaclass	name: #FLWordObjectCluster;	instanceVariables: #().