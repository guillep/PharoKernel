Class	name: #KoreanEnvironment;	superclass: #ClassLanguageEnvironment;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Multilingual-Languages'.Metaclass	name: #KoreanEnvironment;	instanceVariables: #().Metaclass KoreanEnvironment >> traditionalCharsetClass[^ KSX1001]Metaclass KoreanEnvironment >> leadingChar[^ 7]Metaclass KoreanEnvironment >> supportedLanguages[	"Return the languages that this class supports. 	Any translations for those languages will use this class as their environment."^ #('ko')]Metaclass KoreanEnvironment >> systemConverterClass[| encoding |OSPlatform isWin32	ifTrue: [ ^ EUCKRTextConverter ].OSPlatform isMacOS	ifTrue: [ ^ UTF8TextConverter ].OSPlatform isUnix	ifTrue: [ 		encoding := X11Encoding encoding.		encoding ifNil: [ ^ EUCKRTextConverter ].		encoding = 'utf-8'			ifTrue: [ ^ UTF8TextConverter ].		^ EUCKRTextConverter ].^ UTF8TextConverter]