Class	name: #Latin1Environment;	superclass: #ClassLanguageEnvironment;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Multilingual-Languages'.Metaclass	name: #Latin1Environment;	instanceVariables: #().Metaclass Latin1Environment >> nextPutValue: ascii toStream: aStream withShiftSequenceIfNeededForTextConverterState: state[^ self traditionalCharsetClass	nextPutValue: ascii	toStream: aStream	withShiftSequenceIfNeededForTextConverterState: state]Metaclass Latin1Environment >> systemConverterClass[^ Latin1TextConverter]Metaclass Latin1Environment >> nextPutRightHalfValue: ascii toStream: aStream withShiftSequenceIfNeededForTextConverterState: state[^ self traditionalCharsetClass	nextPutRightHalfValue: ascii	toStream: aStream	withShiftSequenceIfNeededForTextConverterState: state]Metaclass Latin1Environment >> supportedLanguages[	"Return the languages that this class supports. 	Any translations for those languages will use this class as their environment."^ #('fr' 'es' 'ca' 'eu' 'pt' 'it' 'sq' 'rm' 'nl' 'de' 'da' 'sv' 'no' 'fi' 'fo' 'is' 'ga' 'gd' 'en' 'af' 'sw')]Metaclass Latin1Environment >> traditionalCharsetClass[^ Latin1]Metaclass Latin1Environment >> leadingChar[^ 0]