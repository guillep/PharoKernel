Class	name: #GreekEnvironment;	superclass: #ClassLanguageEnvironment;	instanceSpecification: #(#pointers #words );	instanceVariables: #();	classVariables: #();	package: #'Multilingual-Languages'.Metaclass	name: #GreekEnvironment;	instanceVariables: #().Metaclass GreekEnvironment >> leadingChar[^ 0]Metaclass GreekEnvironment >> supportedLanguages[	"Return the languages that this class supports. 	Any translations for those languages will use this class as their environment."^ #('el')]Metaclass GreekEnvironment >> systemConverterClass[OSPlatform isWin32	ifTrue: [ ^ CP1253TextConverter ].^ ISO88597TextConverter]