EUCJPTextConverter
	superclass: #EUCTextConverter;
	package: #'Multilingual-TextConversion'.

EUCJPTextConverter >> languageEnvironment
[
	^ JapaneseEnvironment
]

EUCJPTextConverter >> leadingChar
[
	^ JISX0208 leadingChar
]

EUCJPTextConverter class >> encodingNames
[
	^ #('euc-jp' 'eucjp') copy
]

