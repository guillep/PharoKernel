FileSystem class >> inZip: aReference
[
	self deprecated: 'Use #zip:' on: '3 April 2012' in: 'Pharo 1.4'.
	^ self zip: aReference
]

FileSystem class >> zip: aReference
[
	^ self store: (ZipStore reference: aReference)
]

