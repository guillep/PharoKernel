JobChange
	superclass: #Announcement;
	instanceVariables: #(#job );
	package: #Kernel.

JobChange >> job
[
	^ job
]

JobChange >> job: anObject
[
	job := anObject
]

JobChange >> max
[
	^ job max
]

JobChange >> min
[
	^ job min
]

JobChange >> progress
[
	^ job progress
]

JobChange >> title
[
	^ job title
]

JobChange class >> on: aJob
[
	^ self new job: aJob
]

n: aJob
[on: aJob
	^ self new job: aJob
]

