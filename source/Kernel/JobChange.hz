JobChange
	superclass: #Announcement;
	instanceVariables: #(#job );
	package: #Kernel.

JobChange >> job
[job
	^ job
]

JobChange >> job: anObject
[job: anObject
	job := anObject
]

JobChange >> max
[max
	^ job max
]

JobChange >> min
[min
	^ job min
]

JobChange >> progress
[progress
	^ job progress
]

JobChange >> title
[title
	^ job title
]

JobChange class >> on: aJob
[on: aJob
	^ self new job: aJob
]

