PragmaAnnouncement
	superclass: #Announcement;
	instanceVariables: #(#pragma );
	package: #Kernel.

PragmaAnnouncement >> pragma
[pragma
	^ pragma
]

PragmaAnnouncement >> pragma: aPragma
[pragma: aPragma
	pragma := aPragma
]

PragmaAnnouncement class >> pragma: aPragma
[pragma: aPragma
	^ self new pragma: aPragma
]

