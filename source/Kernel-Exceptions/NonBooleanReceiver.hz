Class
	name: #NonBooleanReceiver;
	superclass: #ClassError;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #(#object );
	classVariables: #();
	package: #'Kernel-Exceptions'.

Class NonBooleanReceiver >> isResumable
[
^ true
]

Class NonBooleanReceiver >> object
[
^ object
]

Class NonBooleanReceiver >> object: anObject
[
object := anObject
]

Metaclass
	name: #NonBooleanReceiver;
	instanceVariables: #().

