Class String >> * arg
[
^ arg adaptToString: self andSend: #*
]

Class String >> + arg
[
^ arg adaptToString: self andSend: #+
]

Class String >> - arg
[
^ arg adaptToString: self andSend: #-
]

Class String >> / arg
[
^ arg adaptToString: self andSend: #/
]

Class String >> // arg
[
^ arg adaptToString: self andSend: #//
]

Class String >> \\ arg
[
^ arg adaptToString: self andSend: #\\
]

