String >> * arg
[
	^ arg adaptToString: self andSend: #*
]

String >> + arg
[
	^ arg adaptToString: self andSend: #+
]

String >> - arg
[
	^ arg adaptToString: self andSend: #-
]

String >> / arg
[
	^ arg adaptToString: self andSend: #/
]

String >> // arg
[
	^ arg adaptToString: self andSend: #//
]

String >> \\ arg
[
	^ arg adaptToString: self andSend: #\\
]

