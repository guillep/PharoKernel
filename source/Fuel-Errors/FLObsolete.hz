Class
	name: #FLObsolete;
	superclass: #ClassFLSerializationError;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #();
	classVariables: #();
	package: #'Fuel-Errors'.

Metaclass
	name: #FLObsolete;
	instanceVariables: #().

Metaclass FLObsolete >> signalWithName: classOrTraitName
[
^ self signal: classOrTraitName printString , ' can not be serialized as global because it is obsolete.'
]

