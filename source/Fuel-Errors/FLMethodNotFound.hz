Class
	name: #FLMethodNotFound;
	superclass: #ClassFLMaterializationError;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #();
	classVariables: #();
	package: #'Fuel-Errors'.

Metaclass
	name: #FLMethodNotFound;
	instanceVariables: #().

Metaclass FLMethodNotFound >> signalWith: aGlobalName and: aSelector
[
^ self signal: 'Method ' , aGlobalName , '>>#' , aSelector , ' not found.'
]

