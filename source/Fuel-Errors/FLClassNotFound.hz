Class
	name: #FLClassNotFound;
	superclass: #ClassFLMaterializationError;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #();
	classVariables: #();
	package: #'Fuel-Errors'.

Metaclass
	name: #FLClassNotFound;
	instanceVariables: #().

Metaclass FLClassNotFound >> signalWithName: className
[
^ self signal: 'Class named ' , className printString , ' not found.'
]

