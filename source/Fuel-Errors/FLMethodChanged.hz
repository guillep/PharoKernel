Class
	name: #FLMethodChanged;
	superclass: #ClassFLMaterializationError;
	instanceSpecification: #(#pointers #words );
	instanceVariables: #();
	classVariables: #();
	package: #'Fuel-Errors'.

Metaclass
	name: #FLMethodChanged;
	instanceVariables: #().

Metaclass FLMethodChanged >> signalWith: aGlobalName and: aSelector
[
^ self signal: 'Method ' , aGlobalName , '>>#' , aSelector , ' changed its bytecodes.'
]

