PseudoClassOrganization
	superclass: #ClassOrganization;
	package: #'ClassOrganizer-Core'.

PseudoClassOrganization >> classComment
[
	"Answer the comment associated with the object that refers to the receiver."

	comment ifNil: [ ^ '' ].
	^ comment
]

PseudoClassOrganization >> classComment: aChangeRecord
[
	comment := aChangeRecord
]

