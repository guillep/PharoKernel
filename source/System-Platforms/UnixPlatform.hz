UnixPlatform
	superclass: #OSPlatform;
	package: #'System-Platforms'.

UnixPlatform >> family
[
	^ #Unix
]

UnixPlatform >> isUnix
[
	^ true
]

UnixPlatform >> lineEnding
[
	^ String lf
]

UnixPlatform class >> isActivePlatform
[
	^ self currentPlatformName = 'unix'
]

 self currentPlatformName = 'unix'
]

