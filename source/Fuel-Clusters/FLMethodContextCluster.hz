FLMethodContextCluster
	superclass: #FLVariableObjectCluster;
	package: #'Fuel-Clusters'.

FLMethodContextCluster >> materializeInstanceWith: aDecoder
[
	^ theClass newFromFrameSize: aDecoder nextEncodedByte
]

FLMethodContextCluster >> serializeInstance: aMethodContext with: anEncoder
[
	anEncoder encodeByte: aMethodContext method frameSize
]

