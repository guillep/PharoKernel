RxsEpsilon
	superclass: #RxsNode;
	package: #'Regex-Core'.

RxsEpsilon >> dispatchTo: aBuilder
[dispatchTo: aBuilder
	"Inform the matcher of the kind of the node, and it
	will do whatever it has to."

	^ aBuilder syntaxEpsilon
]

RxsEpsilon >> isNullable
[isNullable
	"See comment in the superclass."

	^ true
]

