Class Time >> fuelAccept: aGeneralMapper
[
^ aGeneralMapper visitHookPrimitive: self
]

Class Time >> serializeOn: anEncoder
[
anEncoder	encodeUint24: self asSeconds;	encodeUint32: self nanoSecond
]

Metaclass Time >> materializeFrom: aDecoder
[
^ self seconds: aDecoder nextEncodedUint24 nanoSeconds: aDecoder nextEncodedUint32
]

