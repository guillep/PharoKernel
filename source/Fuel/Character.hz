Character >> fuelAccept: aGeneralMapper
[fuelAccept: aGeneralMapper
	^ aGeneralMapper visitCharacter: self
]

Character >> serializeOn: anEncoder
[serializeOn: anEncoder
	anEncoder encodeByte: value
]

Character class >> materializeFrom: aDecoder
[materializeFrom: aDecoder
	^ self value: aDecoder nextEncodedByte
]

