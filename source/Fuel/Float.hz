Class Float >> fuelAccept: aGeneralMapper
[
^ aGeneralMapper visitHookPrimitive: self
]

Class Float >> serializeOn: anEncoder
[
anEncoder	encodeUint32: (self at: 1);	encodeUint32: (self at: 2)
]

Metaclass Float >> materializeFrom: aDecoder
[
^ (self new: 2)	at: 1 put: aDecoder nextEncodedUint32;	at: 2 put: aDecoder nextEncodedUint32;	yourself
]

