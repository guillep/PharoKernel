FLBadVersion
	superclass: #FLMaterializationError;
	package: #Fuel.

FLBadVersion class >> signalCurrentVersion: currentVersion streamVersion: streamVersion
[
	^ self
		signal: 'Unexpected stream version ' , streamVersion asString , ' where it should be ' , currentVersion asString , '.'
]

ring , ' where it should be ' , currentVersion asString , '.'
]

